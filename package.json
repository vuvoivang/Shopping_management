{
  "name": "store-project",
  "version": "1.0.0",
  "description": "Demo React + Typescript + SCSS with Training Project KMS",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint",
      "git add ."
    ]
  },
  "scripts": {
    "start": "node config/scripts/start.js",
    "start:a11y": "npm run start -- --env test6 --loglevel debug --loga11y true",
    "start:env": "npm run start -- --env test6 --loglevel debug",
    "start:qa": "cross-env PORT=3333 npm run start:env",
    "start:local": "npm run start -- --env local --loglevel debug",
    "build": "npm install && npm run lint && npm run coverage && node config/scripts/build.js",
    "build:dev": "node config/scripts/build.js",
    "build:check": "npm install && npm run lint && npm run test",
    "format": "prettier --config config/scripts/prettier.config.js --write src/**/*.{js,ts,tsx,scss,json}",
    "lint": "eslint ./src --ext .js,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "esw -w",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit --config config/jest/jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --config config/jest/jest.config.js --watch",
    "coverage": "jest --coverage --config config/jest/jest.config.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.18.6",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@material-ui/core": "^4.12.4",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@reduxjs/toolkit": "1.5.1",
    "@types/redux-mock-store": "^1.0.3",
    "axios": "0.21.1",
    "classnames": "2.2.6",
    "connected-react-router": "6.6.1",
    "core-js": "2.6.10",
    "dompurify": "2.2.8",
    "eventemitter3": "4.0.0",
    "history": "4.10.1",
    "js-cookie": "2.2.1",
    "lodash": "4.17.21",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-chartjs-2": "2.8.0",
    "react-dom": "16.10.2",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "5.2.1",
    "react-intl": "2.8.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-router-hash-link": "2.4.3",
    "react-test-renderer": "^16.14.0",
    "react-toastify": "^9.0.5",
    "recharts": "1.8.5",
    "recompose": "0.30.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "rxjs": "6.5.3",
    "seamless-immutable": "7.1.4",
    "svg.js": "2.7.1",
    "uuid": "8.3.2",
    "value-equal": "1.0.1",
    "yup": "0.27.0"
