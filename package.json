    "webpack": "4.41.4",
    "webpack-dev-server": "3.11.2"
  },
  "sideEffects": false,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "majestic": {
    "jestScriptPath": "./node_modules/jest/bin/jest.js",
    "args": [
      "--runInBand --detectOpenHandles --forceExit --config config/jest/jest.config.js"
    ]
  }
}
